      "source": [
        "I also remove stopwords using the nltk library"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "s0ntV6M1dXop",
        "outputId": "a0c8f63d-e553-4e73-9bed-217506e50065"
      },
      "outputs": [
        {
          "name": "stderr",
          "output_type": "stream",
          "text": [
            "[nltk_data] Downloading package stopwords to /root/nltk_data...\n",
            "[nltk_data]   Package stopwords is already up-to-date!\n"
          ]
        }
      ],
      "source": [
        "import nltk\n",
        "nltk.download('stopwords')\n",
        "from nltk.corpus import stopwords\n",
        "def remove_stopwords(text):\n",
        "    words = text.split()\n",
        "    filtered_words = [word for word in words if word not in stopwords.words('english')]\n",
        "    return ' '.join(filtered_words)\n",
        "test_processed['text'] = test_processed['text'].apply(remove_stopwords)\n",
        "train_processed['text'] = train_processed['text'].apply(remove_stopwords)"
